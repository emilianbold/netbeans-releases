<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="text">
            <folder name="xhtml">
                <folder name="HyperlinkProviders">
                    <file name="ELHyperlinkProvider.instance">
                        <attr name="instanceClass" stringvalue="org.netbeans.modules.web.el.ELHyperlinkProvider"/>
                        <attr name="instanceOf" stringvalue="org.netbeans.lib.editor.hyperlink.spi.HyperlinkProvider"/>
                    </file>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="csl-hints">
        <folder name="text">
            <folder name="x-el">
                <folder name="hints">
                    <folder name="general">
                       <attr name="position" intvalue="100"/>
                        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.el.hints.Bundle"/>
                        <file name="org-netbeans-modules-web-el-hints-ResourceBundleKeys.instance"/>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="OptionsDialog">
        <folder name="Editor">
            <folder name="Hints">
                <attr name="position" intvalue="0"/>
                <folder name="text">
                    <folder name="x-el">
                        <file name="ELHints.instance">
                            <attr name="instanceOf" stringvalue="org.netbeans.spi.options.OptionsPanelController"/>
                            <attr name="instanceCreate" methodvalue="org.netbeans.modules.web.el.hints.HintsAdvancedOption.createStatic"/>
                        </file>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>

</filesystem>
