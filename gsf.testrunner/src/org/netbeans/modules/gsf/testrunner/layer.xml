<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <!-- actions -->
    <folder name="Actions">
        <folder name="Window">
            <file name="org-netbeans-modules-gsf-testrunner-api-ResultWindowOpenAction.instance">
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/gsf/testrunner/resources/testResults.png"/>
            </file>
        </folder> <!-- Window -->
        <folder name="UnitTests">
            <file name="org-netbeans-modules-gsf-testrunner-TestCreatorAction.instance"/>
        </folder>

    </folder>
    <folder name="Menu">
        <folder name="Window">
            <folder name="Tools">
                <file name="GsfTestRunnerResultWindowOpenAction.shadow">
                    <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-gsf-testrunner-api-ResultWindowOpenAction.instance"/>
                    <attr name="position" intvalue="250"/>
                </file>
            </folder>
        </folder> <!-- Window -->
    </folder>
    <folder name="Windows2">
        <folder name="Modes">
            <folder name="output">        <!-- "output" is defined in core/ui -->
                <file name="gsf-testrunner-results.wstcref" url="gsf-testrunner-results.wstcref">
                    <attr name="position" intvalue="1950"/>
                </file>
            </folder>
        </folder>
        <folder name="Components">
            <file name="gsf-testrunner-results.settings" url="gsf-testrunner-results.settings"/>
        </folder>
    </folder>

    <folder name="Templates">
        <folder name="UnitTests">
            <attr name="displayName" bundlevalue="org.netbeans.modules.gsf.testrunner.Bundle#UnitTests_displayname"/>
            <attr name="position" intvalue="1200"/>
            
        </folder>
    </folder>

    <!-- shortcuts -->
    <folder name="Shortcuts">
        <file name="DS-U.shadow">
            <attr name="originalFile" stringvalue="Actions/UnitTests/org-netbeans-modules-gsf-testrunner-TestCreatorAction.instance"/>
        </file>
        <file name="AS-R.shadow">
            <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-gsf-testrunner-api-ResultWindowOpenAction.instance"/>
        </file>
    </folder>
</filesystem>
