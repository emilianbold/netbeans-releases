<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>

    <folder name="Menu">
        <folder name="BuildProject">
            <attr name="SeparatorCompileSingle.instance/org-apache-tools-ant-module-run-StopBuildingAction.instance" boolvalue="true"/>
            <file name="org-apache-tools-ant-module-run-StopBuildingAction.instance"/>
            <attr name="org-apache-tools-ant-module-run-StopBuildingAction.instance/SeparatorStopBuilding.instance" boolvalue="true"/>
            <file name="SeparatorStopBuilding.instance">
                <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
            </file>
            <attr name="SeparatorStopBuilding.instance/org-netbeans-core-actions-JumpNextAction.shadow" boolvalue="true"/>
        </folder>
    </folder>

  <folder name="Templates">
 
    <attr name="Package/Ant" boolvalue="true"/>
    <folder name="Ant">
      <attr name="SystemFileSystem.localizingBundle" stringvalue="org.apache.tools.ant.module.resources.Bundle"/>
      <attr name="templateWizardURL" urlvalue="nbresloc:/org/apache/tools/ant/module/resources/AntDescription.html"/>
      <file name="Project.xml" url="Project.xml_">
        <attr name="template" boolvalue="true"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.apache.tools.ant.module.resources.Bundle"/>
        <attr name="templateWizardURL" urlvalue="nbresloc:/org/apache/tools/ant/module/resources/ProjectDescription.html"/>
        <attr name="templateCategory" stringvalue="ant-script"/>
      </file>
      <attr name="Project.xml/CustomTask.java" boolvalue="true"/>
      <file name="CustomTask.java" url="CustomTask_java">
        <attr name="template" boolvalue="true"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.apache.tools.ant.module.resources.Bundle"/>
        <!-- XXX #44054: implies a runtime dep on java/project module: -->
        <attr name="instantiatingIterator" methodvalue="org.netbeans.spi.java.project.support.ui.templates.JavaTemplates.createJavaTemplateIterator"/>
        <attr name="instantiatingIteratorURL" urlvalue="nbresloc:/org/apache/tools/ant/module/resources/CustomTaskDescription.html"/>
        <attr name="templateCategory" stringvalue="ant-task"/>
      </file>
    </folder>
    <attr name="Classes/Ant" boolvalue="true"/>
    <attr name="Ant/Other" boolvalue="true"/>

  </folder>

  <folder name="Services">

    <file name="org-apache-tools-ant-module-AntSettings.settings" url="AntSettings.settings">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.apache.tools.ant.module.resources.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/apache/tools/ant/module/resources/AntIcon.gif"/>
    </file>

    <!-- Recognize text/x-ant+xml: -->
    <folder name="MIMEResolver">
      <file name="org-apache-tools-ant-module-mime-resolver.xml" url="mime-resolver.xml">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.apache.tools.ant.module.resources.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/apache/tools/ant/module/resources/AntIcon.gif"/>
      </file>
    </folder>

    <folder name="Hidden">
      <!-- To make the module nicer to debug, list the cookies it provides. -->
      <file name="org-apache-tools-ant-module-cookies.xml" url="cookies.xml"/>
    </folder>

  </folder>

  <folder name="UI">

    <folder name="Services">

      <folder name="Building">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.apache.tools.ant.module.resources.Bundle"/>
        <file name="org-apache-tools-ant-module-AntSettings.shadow">
          <attr name="originalFile" stringvalue="Services/org-apache-tools-ant-module-AntSettings.settings"/>
        </file>
      </folder> <!-- Building -->

    </folder> <!-- Services -->

  </folder> <!-- UI -->

  <!-- Lines marked as erroneous: -->
  <folder name="Editors">
    <folder name="AnnotationTypes">
      <file name="org-apache-tools-ant-module-error.xml" url="error-annotation.xml"/>
    </folder>
  </folder>

</filesystem>
