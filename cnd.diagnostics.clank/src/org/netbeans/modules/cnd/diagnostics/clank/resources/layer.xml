<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Editors"> 
        <folder name="AnnotationTypes">
            <file name="ErrorDiagnosticPin.xml" url="ErrorDiagnosticPin.xml"/>
            <file name="WarningDiagnosticPin.xml" url="WarningDiagnosticPin.xml"/>            
            <file name="clank-diagnostics-annotations" url="clank-diagnostics-annotations.xml"/>
            <file name="clank-diagnostics-annotations-fixable" url="clank-diagnostics-annotations-fixable.xml"/>
            <file name="clank-diagnostics-annotations-warning" url="clank-diagnostics-annotations-warning.xml"/>
            <file name="clank-diagnostics-annotations-warning-fixable" url="clank-diagnostics-annotations-warning-fixable.xml"/>
        </folder> 
        <folder name="FontsColors">
            <folder name="NetBeans">
                <folder name="Defaults">
                    <file name="diagnostics-annotationsupport-fontsColors.xml" url="fontsColors.xml">
<!--                        <attr name="SystemFileSystem.localizingBundle" stringvalue="com.sun.tools.analytics.ui.resources.Bundle"/>-->
                        <attr name="nbeditor-settings-ColoringType" stringvalue="highlight"/>
                    </file>
                </folder>
            </folder>
        </folder>   
        <folder name="Preferences">
            <folder name="Defaults">
                <file name="preferences-for-diagnostics.xml" url="editor-preferences.xml">
<!--                    <attr name="position" intvalue="10000"/>-->
                </file>
                <!-- our file have to be after org-netbeans-modules-editor-preferences*.xml -->
                <!-- they don't use positions yet, let's use relative position then -->
<!--                <attr name="org-netbeans-modules-editor-preferences.xml/preferences-for-diagnostics.xml" boolvalue="true"/>
                <attr name="org-netbeans-modules-editor-preferences-mac.xml/preferences-for-diagnostics.xml" boolvalue="true"/>-->
            </folder>
        </folder>                
    </folder>   
</filesystem>
