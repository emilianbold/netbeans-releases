                <package>org.netbeans.api.db.explorer.support</package>
                <package>org.netbeans.spi.db.explorer</package>
            </public-packages>
            <class-path-extension>
                <runtime-relative-path>ext/ddl.jar</runtime-relative-path>
            </class-path-extension>
            <extra-compilation-unit>
                <package-root>libsrc</package-root>
                <classpath>${lib.cp}</classpath>
                <built-to>build/lib-classes</built-to>
                <built-to>${cluster}/modules/ext/ddl.jar</built-to>
            </extra-compilation-unit>
        </data>
        <junit-version xmlns="http://www.netbeans.org/ns/junit/1" value="junit3"/>
