	/**
	* Root section.
	* This section describes subnode "Databases" in "Runtime". It is a root for
	* database explorer, handles open connections and a list of drives. Serves list
	* of supported database products (suppdbsystems). Allows to add a new connection.
	*/
	root = {
		name = Databases;
		class = org.netbeans.modules.db.explorer.nodes.RootNode;
		infoclass = org.netbeans.modules.db.explorer.infos.RootNodeInfo;
		children_ordering = {
			org.openide.loaders.DataFolder$FolderNode = 0;
                        org.netbeans.modules.db.mysql.nodes.ServerNode = 1;
                        org.netbeans.modules.derby.DerbyServerNode = 2;
			org.netbeans.modules.db.explorer.nodes.DriverListNode = 3;
			org.netbeans.modules.db.explorer.nodes.ConnectionNode = 4;
		};
		iconbase = "org/netbeans/modules/db/resources/database";
		children = (driverlist);
		properties = ();
		options = {};
		actions = (
			{
				name = "AddConnection";
				class = org.netbeans.modules.db.explorer.actions.AddConnectionAction;
			},
                        delimiter,
			{
				name = "EnableDebug";
				class = org.netbeans.modules.db.explorer.actions.EnableDebugAction;
			},
			{
				name = "DisableDebug";
				class = org.netbeans.modules.db.explorer.actions.DisableDebugAction;
			}
		);
	};

	/**
	* Driver list section.
	* This section describes subnode "Drivers" in "Database". It is a root for driver
	* list (a list of all available and registered drivers). Allows add a new driver.
	* Serves no properties (name isn't modifable).
	*/
