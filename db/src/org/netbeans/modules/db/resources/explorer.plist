		name = Drivers;
		class = org.netbeans.modules.db.explorer.nodes.DriverListNode;
		infoclass = org.netbeans.modules.db.explorer.infos.DriverListNodeInfo;
		iconbase = "org/netbeans/modules/db/resources/defaultFolder";
		children = ();
		properties = ();
		options = {};
		actions = (
			{
				name = "AddNewDriver";
				class = org.netbeans.modules.db.explorer.actions.AddDriverAction;
			},
			delimiter
		);
	};

