      else
        |>METHOD:raise<| ArgumentError.new("cannot interpret as IPv6 address: #{|>PARAMETER:arg<|.inspect}")
      end
    end

    def |>METHOD:initialize<|(|>PARAMETER:address<|)
      unless |>PARAMETER:address<|.kind_of?(String) && |>PARAMETER:address<|.length == 16
