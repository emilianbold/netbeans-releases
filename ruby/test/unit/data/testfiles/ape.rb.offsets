      <FCALLNODE>good<STRNODE> <ARRAYNODE><DSTRNODE>"Client-provided slug '</STRNODE><EVSTRNODE>#{<LOCALVARNODE>slug</LOCALVARNODE>}</EVSTRNODE><STRNODE>' was used in Media Resource URI.</STRNODE>"</DSTRNODE></ARRAYNODE></FCALLNODE>
    else
