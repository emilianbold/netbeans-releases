      <BLOCKNODE><INSTASGNNODE>@filename = <LOCALVARNODE>filename</LOCALVARNODE></INSTASGNNODE>
      <INSTASGNNODE>@mutex = <CALLNODE><CONSTNODE>Mutex</CONSTNODE>.new</CALLNODE></INSTASGNNODE>
