    <DEFNNODE>def <ARGUMENTNODE>lazy_initialize<ARGSNODE></ARGSNODE></ARGUMENTNODE>
      <BLOCKNODE><CALLNODE><INSTVARNODE>@mutex</INSTVARNODE>.synchronize <ITERNODE>{
