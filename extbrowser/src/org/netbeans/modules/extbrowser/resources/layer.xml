<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
  <folder name="Services">
    <folder name="Browsers">

      <!-- Present on Non-Windows, Non-Unix platforms, like OS_2, Mac OS -->
      <file name="SimpleExtBrowser.settings" url="simpleBrowser.xml">
        <attr name="hidden" methodvalue="org.netbeans.modules.extbrowser.SimpleExtBrowser.isHidden"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.extbrowser.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbres:/org/netbeans/modules/extbrowser/resources/extbrowser.gif"/>
      </file>

      <!-- Present on Windows or if running on JDK 6 and Desktop.isDesktopSupported returns true -->
      <file name="SystemDefaultBrowser.settings" url="SystemDefaultBrowser.xml">
        <attr name="hidden" methodvalue="org.netbeans.modules.extbrowser.SystemDefaultBrowser.isHidden"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.extbrowser.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbres:/org/netbeans/modules/extbrowser/resources/extbrowser.gif"/>
      </file>

      <file name="IExplorerBrowser.settings" url="IExplorerBrowser.xml">
        <attr name="hidden" methodvalue="org.netbeans.modules.extbrowser.IExplorerBrowser.isHidden"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.extbrowser.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbres:/org/netbeans/modules/extbrowser/resources/extbrowser.gif"/>
      </file>

      <!-- Present on Unix & Windows -->
      <file name="NetscapeBrowser.settings" url="NetscapeBrowser.xml">
        <attr name="hidden" methodvalue="org.netbeans.modules.extbrowser.NetscapeBrowser.isHidden"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.extbrowser.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbres:/org/netbeans/modules/extbrowser/resources/extbrowser.gif"/>
      </file>

      <file name="MozillaBrowser.settings" url="MozillaBrowser.xml">
        <attr name="hidden" methodvalue="org.netbeans.modules.extbrowser.MozillaBrowser.isHidden"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.extbrowser.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbres:/org/netbeans/modules/extbrowser/resources/extbrowser.gif"/>
      </file>

      <file name="FirefoxBrowser.settings" url="FirefoxBrowser.xml">
        <attr name="hidden" methodvalue="org.netbeans.modules.extbrowser.FirefoxBrowser.isHidden"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.extbrowser.Bundle"/>
        <attr name="SystemFileSystem.icon" urlvalue="nbres:/org/netbeans/modules/extbrowser/resources/extbrowser.gif"/>
      </file>

      <!-- Default system browser should be first on Windows -->
      <attr name="SystemDefaultBrowser.settings/SimpleExtBrowser.settings" boolvalue="true"/>
      <attr name="SystemDefaultBrowser.settings/MozillaBrowser.settings" boolvalue="true"/>
      <attr name="SystemDefaultBrowser.settings/NetscapeBrowser.settings" boolvalue="true"/>
      <attr name="SystemDefaultBrowser.settings/IExplorerBrowser.settings" boolvalue="true"/>
      <attr name="SystemDefaultBrowser.settings/FirefoxBrowser.settings" boolvalue="true"/>

      <!-- Browsers order -->
      <attr name="MozillaBrowser.settings/SimpleExtBrowser.settings" boolvalue="true"/>
      <attr name="NetscapeBrowser.settings/SimpleExtBrowser.settings" boolvalue="true"/>
      <attr name="IExplorerBrowser.settings/SimpleExtBrowser.settings" boolvalue="true"/>
      <attr name="FirefoxBrowser.settings/SimpleExtBrowser.settings" boolvalue="true"/>

      <attr name="IExplorerBrowser.settings/FirefoxBrowser.settings" boolvalue="true"/>
      <attr name="FirefoxBrowser.settings/MozillaBrowser.settings" boolvalue="true"/>
      <attr name="MozillaBrowser.settings/NetscapeBrowser.settings" boolvalue="true"/>

      <!-- Other browsers order -->
      <attr name="SimpleExtBrowser.settings/SwingBrowser.settings" boolvalue="true"/>
            
     </folder>
  </folder>

  <folder name="Templates">
    <folder name="Services">
      <folder name="Browsers">
      
        <file name="ExtWebBrowser.settings" url="externalBrowser.xml">
          <attr name="template" boolvalue="true"/>
          <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.extbrowser.Bundle"/>
          <attr name="SystemFileSystem.icon" urlvalue="nbres:/org/netbeans/modules/extbrowser/resources/extbrowser.gif"/>
        </file>
        
      </folder>
    </folder>
  </folder>
</filesystem>
