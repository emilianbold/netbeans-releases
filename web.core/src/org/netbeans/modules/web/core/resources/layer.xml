<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>

  <folder name="Templates">
    <attr name="JSP_Servlet/Classes" boolvalue="true"/>
    <folder name="JSP_Servlet">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
        <attr name="templateWizardURL" 
              urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/Web.html"/>
      <attr name="JSP.jsp/Servlet.java" boolvalue="true"/>
      <attr name="Servlet.java/SimpleFilter.java" boolvalue="true"/>
      <attr name="SimpleFilter.java/ServletListener.java" boolvalue="true"/>
      <attr name="ServletListener.java/TagLibrary.tld" boolvalue="true"/>
      <attr name="TagLibrary.tld/TagFile.tag" boolvalue="true"/>
      <attr name="TagFile.tag/TagHandler.java" boolvalue="true"/>
      <attr name="TagHandler.java/Html.html" boolvalue="true"/>
      <file name="JSP.jsp" url="templates/JSP.template">
        <attr name="template" boolvalue="true"/>
        <attr name="templateWizardURL" 
              urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/JSP.html"/>
        <attr name="SystemFileSystem.localizingBundle"
                    stringvalue="org.netbeans.modules.web.core.Bundle"/>
        <attr name="templateWizardIterator" 
              methodvalue="org.netbeans.modules.web.wizards.PageIterator.createJspIterator"/>
        <attr name="templateCategory" stringvalue="web-types"/>
      </file>
      <!--  JSP Document --> 
      <file name="JSPX.jspx" url="templates/JSPDoc.template"/>
      <!--  JSP Segment --> 
      <file name="JSPF.jspf" url="templates/JSPF.template"/>
      <!--  XML segemnt --> 
      <file name="JSPFX.jspf" url="templates/JSPDocX.template"/>
      <!-- Tag File -->
      <file name="TagFile.tag" url="templates/TagFile.template">
        <attr name="template" boolvalue="true"/>
        <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/TagFile.html"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
        <attr name="templateWizardIterator" 
            methodvalue="org.netbeans.modules.web.wizards.PageIterator.createTagIterator"/>
        <attr name="templateCategory" stringvalue="servlet-types"/>
      </file>
      <!--  Tag File Document --> 
      <file name="TagFileX.tagx" url="templates/TagFileX.template"/>
      <!--  Tag File Segments --> 
      <file name="TagFileF.tagf" url="templates/TagFileF.template"/>
      <file name="TagFileFX.tagf" url="templates/TagFileFX.template"/>
      <!-- Servlet --> 
      <file name="Servlet.java" url="templates/Servlet.template">
          <attr name="template" boolvalue="true"/>
          <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/Servlet.html"/>
          <attr name="templateWizardIterator" methodvalue="org.netbeans.modules.web.wizards.ServletIterator.createServletIterator"/>
          <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
          <attr name="org.netbeans.modules.web.IsServletFile" boolvalue="true"/>
          <attr name="templateCategory" stringvalue="servlet-types"/>
      </file>
      <!-- use template description from HTML module -->
      <file name="Html.html" url="nbresloc:/org/netbeans/modules/html/templates/html.html">
         <attr name="template" boolvalue="true" />
         <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/html/TemplateHelp.html" />
         <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
         <attr name="templateWizardIterator" 
                methodvalue="org.netbeans.modules.web.wizards.PageIterator.createHtmlIterator"/>
         <attr name="templateCategory" stringvalue="web-types"/>
      </file>
      <!-- Servlet Listener --> 
      <file name="ServletListener.java" url="templates/ServletListener.template">
          <attr name="template" boolvalue="true"/>
          <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/ServletListener.html"/>
          <attr name="templateWizardIterator" newvalue="org.netbeans.modules.web.wizards.ListenerIterator"/>
          <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
          <attr name="templateCategory" stringvalue="servlet-types"/>
      </file>
      <!-- Tag Library 2.0--> 
      <file name="TagLibrary.tld" url="templates/BlankTagLibrary.template">
          <attr name="template" boolvalue="true"/>
          <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/TagLibrary.html"/>
          <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
          <attr name="templateWizardIterator" 
                methodvalue="org.netbeans.modules.web.wizards.PageIterator.createTagLibraryIterator"/>
          <attr name="templateCategory" stringvalue="servlet-types"/>
      </file>
      <!--  Tag Library 1.2 --> 
      <file name="TagLibrary_1_2.tld" url="templates/BlankTagLibrary_1_2.template"/>
      <!-- Filters --> 
      <file name="SimpleFilter.java" url="templates/SimpleFilter.template">
        <attr name="template" boolvalue="true"/>
        <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/SimpleFilter.html"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
        <attr name="templateWizardIterator" methodvalue="org.netbeans.modules.web.wizards.ServletIterator.createFilterIterator"/>
        <attr name="templateCategory" stringvalue="servlet-types"/>
      </file>      
      <file name="AdvancedFilter.java" url="templates/AdvancedFilter.template"/>
      <!-- Tag Handlers --> 
      <file name="TagHandler.java" url="templates/SimpleTagHandler.template">
        <attr name="template" boolvalue="true"/>
        <attr name="templateWizardURL" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/TagHandler.html"/>
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle"/>
        <attr name="templateWizardIterator" newvalue="org.netbeans.modules.web.wizards.TagHandlerIterator"/>
        <attr name="templateCategory" stringvalue="servlet-types"/>
      </file>      
      <file name="BodyTagHandler.java" url="templates/BodyTagHandler.template"/>
      
    </folder>
  </folder>

  <folder name="Actions">
    <folder name="Build">
      <file name="Set Request Parameters.instance">
        <attr name="instanceClass" stringvalue="org.netbeans.modules.web.core.EditQueryStringAction"/>
      </file>
    </folder>
    <folder name="System">
      <file name="View Servlet.instance">
        <attr name="instanceClass" stringvalue="org.netbeans.modules.web.core.jsploader.EditServletAction"/>
      </file>
    </folder>
  </folder>

  <folder name="Services">
  <!--
    <folder name="Hidden">
	    <attr name="hidden" boolvalue="true" />
	    <file name="org-netbeans-modules-web-core-resources-EntityCatalog.xml" url="entityCatalog.xml"/>
    </folder>
  -->
    <!-- Recognize text/x-jsp -->
    <folder name="MIMEResolver">
      <file name="org-netbeans-modules-web-core-resources-mime-resolver.xml" url="mime-resolver.xml">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.Bundle" />
        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/web/core/resources/servletSettings.gif" />
      </file>
    </folder>

  </folder>

  <folder name="J2EE">
    <folder name="JSPParser">
    </folder>
    <folder name="JSPCompilation">
    </folder>
    <folder name="JSPSyntaxColoring">
        <file name="org-netbeans-modules-web-core-jsploader-JspContextInfoImpl.instance">
            <attr name="instanceCreate" newvalue="org.netbeans.modules.web.core.jsploader.JspContextInfoImpl" />
        </file>
    </folder>
  </folder>
  
  <folder name="Editors">
        <folder name="text">
            <folder name="x-jsp">
                <folder name="Popup">
                    
                    <file name="org-netbeans-modules-web-core-jsploader-EditServletAction.shadow">
                        <attr name="originalFile" stringvalue="Actions/System/View Servlet.instance"/>
                    </file>

                    <file name="org-netbeans-modules-project-ui-RunSingle.instance">
                        <attr name="instanceCreate" methodvalue="org.netbeans.modules.project.ui.actions.Actions.runSingle"/>
                    </file>
                    
                    <attr name="Separator5.instance/org-netbeans-modules-project-ui-RunSingle.instance" boolvalue="true"/>
                    <attr name="org-netbeans-modules-project-ui-RunSingle.instance/org-netbeans-modules-web-core-jsploader-EditServletAction.shadow" boolvalue="true"/>
                    <attr name="org-netbeans-modules-web-core-jsploader-EditServletAction.shadow/SeparatorJSP.instance" boolvalue="true"/>

                </folder>
            </folder>
        </folder>
  </folder>
    
  <folder name="Shortcuts">
    <file name="C-Q.instance">
      <attr name="instanceClass" stringvalue="org.netbeans.modules.web.core.EditQueryStringAction"/>
    </file>
  </folder>      

    <folder name="Plugins">
      <!-- Register XML module catalog plugin -->
        <folder name="XML">
            <folder name="UserCatalogs">
                <file name="org-netbeans-modules-web-taglib-TaglibCatalog.instance">
                    <attr name="instanceCreate" 
                        newvalue="org.netbeans.modules.web.taglib.TaglibCatalog"/>
                    <attr name="instanceOf" 
                        stringvalue="org.netbeans.modules.xml.catalog.spi.CatalogReader"/>
                </file>
            </folder>
            <folder name="GrammarQueryManagers">
                <!-- register semantics grammar for jsp_taglibrary2_0 due to code completion -->
                <file name="org-netbeans-modules-web-taglib-TaglibGrammarQueryManager.instance"/>
                <attr name="org-netbeans-modules-web-taglib-TaglibGrammarQueryManager.instance/semantics-grammar-to-generic-grammar-separator" boolvalue="true"/>
            </folder> <!-- GrammarQueryManagers -->
        </folder>
    </folder>

    <folder name="JSPPalette">

        <folder name="HTML.shadow">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.palette.Bundle"/>
            <attr name="originalFile" stringvalue="HTMLPalette/HTML"/>
        </folder>
        
        <folder name="HTML_Forms.shadow">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.palette.Bundle"/>
            <attr name="originalFile" stringvalue="HTMLPalette/HTML_Forms"/>
        </folder>

        <folder name="JSP">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.web.core.palette.Bundle"/>

            <file name="UseBean.xml" url="nbresloc:/org/netbeans/modules/web/core/palette/items/resources/UseBean.xml" />
