<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <managed-bean>
        <managed-bean-name>SessionBean1</managed-bean-name>
        <managed-bean-class>twopagecrudtable.SessionBean1</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Page1</managed-bean-name>
        <managed-bean-class>twopagecrudtable.Page1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RequestBean1</managed-bean-name>
        <managed-bean-class>twopagecrudtable.RequestBean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean1</managed-bean-name>
        <managed-bean-class>twopagecrudtable.ApplicationBean1</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Masthead</managed-bean-name>
        <managed-bean-class>twopagecrudtable.Masthead</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>CreatePage</managed-bean-name>
        <managed-bean-class>twopagecrudtable.CreatePage</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>UpdatePage</managed-bean-name>
        <managed-bean-class>twopagecrudtable.UpdatePage</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/Page1.jsp</from-view-id>
        <navigation-case>
            <from-outcome>createCase</from-outcome>
            <to-view-id>/CreatePage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>updateCase</from-outcome>
            <to-view-id>/UpdatePage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/CreatePage.jsp</from-view-id>
        <navigation-case>
            <from-outcome>created</from-outcome>
            <to-view-id>/Page1.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>canceled</from-outcome>
            <to-view-id>/Page1.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/UpdatePage.jsp</from-view-id>
        <navigation-case>
            <from-outcome>updated</from-outcome>
            <to-view-id>/Page1.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>noUpdate</from-outcome>
            <to-view-id>/Page1.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
