<mconfig name="projects">
    
    <testbag testattribs="validation" name="Basic" executor="ide">
        <testset dir="qa-functional/src">
            <patternset>
                <include name="projects/TestCreateProject.class"/>
                <exclude name="projects/TestFreeformProject.class"/>
                <exclude name="projects/TestCreateProject.class/testCreateProject_6"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="test" name="Test" executor="ide">
        <testset dir="qa-functional/src">
            <patternset>
                <include name="projects/TestCreateProject.class/testCreateProject_6"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- For validation run exclude TestLibraries.testDefaultLibraries test, 
         was intended only between Beta2 and FCS of 4.0 -->
    <testbag testattribs="libraries" name="Libraries" executor="ide">
        <testset dir="qa-functional/src">
            <patternset>
                <include name="projects/TestLibraries.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="platforms" name="Platforms" executor="ide">
        <testset dir="qa-functional/src">
            <patternset>
                <include name="projects/TestCreatePlatforms.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <executor name="ide" antfile="build-qa-functional.xml" target="runidetest" default="true"/>
    <compiler name="compiler" antfile="build-qa-functional.xml" target="qa-functional-compiler" default="true"/>
    
</mconfig>
