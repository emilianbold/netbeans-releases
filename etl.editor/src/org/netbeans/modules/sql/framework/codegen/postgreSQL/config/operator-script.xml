<?xml version="1.0"  encoding="ISO-8859-1"?>
<!--
  Definition for Operators that are supported by SeeBeyond SQL Builder
-->
<operator-script>
    <operators>
        <operator name="now" dbspecificname="SYSDATE" script="CURRENT_TIMESTAMP" type="timestamp">
            <output type="timestamp" />
        </operator>
        <operator name="numberToHex" dbspecificname="Hex" script="to_hex($val)" type="string">
            <arg name="val" type="integer" />
            <output type="varchar" />
        </operator>        
    </operators>    
</operator-script>
