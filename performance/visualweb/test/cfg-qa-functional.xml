<mconfig name="core qa-performance test config">
    
    <!-- testbag common for all suites to setup usedir -->
    <testbag testattribs="all or menus or dialogs or actions or setup or editor_related or java_related or apisupport_related or temp" prio="1"
             executor="ide_perf_setup" name="UI responsiveness - setup">
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasuringSetup.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- 1st round -->    
    <testbag testattribs="all or actions" prio="2"
             executor="ide_performance" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureActions.class"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="all or dialogs" prio="3"
             executor="ide_performance" name="UI responsiveness - Dialogs" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureDialogs.class"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="all or menus" prio="4"
             executor="ide_performance" name="UI responsiveness - Menus" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureMenus.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- 2nd round -->    
    <testbag testattribs="(all or actions) and NOT once" prio="5"
             executor="ide_performance" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureActions.class"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="(all or dialogs) and NOT once" prio="6"
             executor="ide_performance" name="UI responsiveness - Dialogs" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureDialogs.class"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="(all or menus) and NOT once" prio="7"
             executor="ide_performance" name="UI responsiveness - Menus" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureMenus.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- 3rd round -->    
    <testbag testattribs="(all or actions) and NOT once" prio="8"
             executor="ide_performance" name="UI responsiveness - Actions" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureActions.class"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="(all or dialogs) and NOT once" prio="9"
             executor="ide_performance" name="UI responsiveness - Dialogs" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureDialogs.class"/>
            </patternset>
        </testset>
    </testbag>
    <testbag testattribs="(all or menus) and NOT once" prio="10"
             executor="ide_performance" name="UI responsiveness - Menus" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureMenus.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- Scan Classpath test bag -->    
    <testbag testattribs="all or classpath_scan" prio="100"
             executor="ide_performance_scan" name="Classpath Scan" >
        <testset dir="qa-functional/src">
            <patternset>
                
                <include name="gui/scanclasspath/WebScanClasspath.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- Custom / Module test bags -->    
    <testbag testattribs="java_related" prio="100"
             executor="ide_performance" name="UI responsiveness of actions, dialogs or menus related to java, editor, mdr" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureJava.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="editor_related" prio="105"
             executor="ide_performance" name="UI responsiveness of actions, dialogs or menus related to editor" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureEditor.class"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- Temp -->    
    <testbag testattribs="temp" prio="1000"
             executor="ide_performance" name="Temp" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="gui/MeasureTemp.class"/>
            </patternset>
        </testset>
    </testbag>
    
<!-- Memory Footprint Testbags -->
    <!-- Out Of The Box -->
    <testbag testattribs="footprint_all or out_of_the_box_startup" prio="300"
             executor="ide_footprint" name="Footprint - Out Of The Box Startup" >
        <testset dir="qa-functional/src">
            <patternset>
                <include name="footprint/VWPFootprintTests.class/testOutOfTheBoxStartup"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(footprint_all or out_of_the_box_startup) and NOT once" prio="301"
             executor="ide_footprint" name="Footprint - Out Of The Box Startup" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/VWPFootprintTests.class/testOutOfTheBoxStartup"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(footprint_all or out_of_the_box_startup) and NOT once" prio="302"
             executor="ide_footprint" name="Footprint - Out Of The Box Startup" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/VWPFootprintTests.class/testOutOfTheBoxStartup"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- J2SE Project workflow -->
    <testbag testattribs="footprint_all or j2se_project" prio="310"
             executor="ide_footprint" name="Footprint - J2SE project workflow" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/VWPFootprintTests.class/testJ2SEProjectWorkflow"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(footprint_all or j2se_project) and NOT once" prio="311"
             executor="ide_footprint" name="Footprint - J2SE project workflow" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/VWPFootprintTests.class/testJ2SEProjectWorkflow"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(footprint_all or j2se_project) and NOT once" prio="312"
             executor="ide_footprint" name="Footprint - J2SE project workflow" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/VWPFootprintTests.class/testJ2SEProjectWorkflow"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- Web Project workflow -->
    <testbag testattribs="footprint_all or web_project" prio="320"
             executor="ide_footprint" name="Footprint - Web project workflow" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/VWPFootprintTests.class/testWebProjectWorkflow"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(footprint_all or web_project) and NOT once" prio="321"
             executor="ide_footprint" name="Footprint - Web project workflow" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/VWPFootprintTests.class/testWebProjectWorkflow"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(footprint_all or web_project) and NOT once" prio="322"
             executor="ide_footprint" name="Footprint - Web project workflow" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/VWPFootprintTests.class/testWebProjectWorkflow"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- Visual Web Project workflow -->
    <testbag testattribs="footprint_all or visualweb_project" prio="330"
             executor="ide_footprint" name="Footprint - Visual Web project workflow" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/VWPFootprintTests.class/testVisualWebProjectWorkflow"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(footprint_all or visualweb_project) and NOT once" prio="331"
             executor="ide_footprint" name="Footprint - Visual Web project workflow" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/VWPFootprintTests.class/testVisualWebProjectWorkflow"/>
            </patternset>
        </testset>
    </testbag>
    
    <testbag testattribs="(footprint_all or visualweb_project) and NOT once" prio="332"
             executor="ide_footprint" name="Footprint - Visual Web project workflow" >
        <testset dir="footprint/src">
            <patternset>
                <include name="footprint/VWPFootprintTests.class/testVisualWebProjectWorkflow"/>
            </patternset>
        </testset>
    </testbag>
    
    <!-- executors & compilers -->
    <executor name="ide_perf_setup" antfile="build-qa-functional.xml" target="runidesetup"/>
    <executor name="ide_performance" antfile="build-qa-functional.xml" target="runidetest" default="true"/>
    <executor name="ide_performance_scan" antfile="build-qa-functional.xml" target="runidetest_scan" />
    <executor name="ide_footprint" antfile="build-qa-functional.xml" target="runidetest_footprint"/>
    
    <compiler name="compiler" antfile="build-qa-functional.xml" target="qa-functional-compiler" default="true"/>
    
</mconfig>
