<mconfig name="core qa-performance test config">

    <!-- testbag common for all suites to setup usedir -->
    <testbag testattribs="measure_ui or measure_menus or measure_dialogs or measure_actions or measure_setup or temp" prio="1"
             executor="ide_perf_setup" name="Setup IDE for UI responsiveness">
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasuringSetup.class"/>
	    </patternset>
	</testset>
    </testbag>

    <!-- 1st round -->    
    <testbag testattribs="measure_ui or measure_actions" prio="2"
             executor="ide_performance" name="Measure UI responsiveness of actions" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureActions.class"/>
	    </patternset>
	</testset>
    </testbag>
    <testbag testattribs="measure_ui or measure_dialogs" prio="3"
             executor="ide_performance" name="Measure UI responsiveness of dialogs" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureDialogs.class"/>
	    </patternset>
	</testset>
    </testbag>
    <testbag testattribs="measure_ui or measure_menus" prio="4"
             executor="ide_performance" name="Measure UI responsiveness of menus" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureMenus.class"/>
	    </patternset>
	</testset>
    </testbag>
    
    <!-- 2nd round -->    
    <testbag testattribs="(measure_ui or measure_actions) and NOT measure_once" prio="5"
             executor="ide_performance" name="Measure UI responsiveness of actions" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureActions.class"/>
	    </patternset>
	</testset>
    </testbag>
    <testbag testattribs="(measure_ui or measure_dialogs) and NOT measure_once" prio="6"
             executor="ide_performance" name="Measure UI responsiveness of dialogs" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureDialogs.class"/>
	    </patternset>
	</testset>
    </testbag>
    <testbag testattribs="(measure_ui or measure_menus) and NOT measure_once" prio="7"
             executor="ide_performance" name="Measure UI responsiveness of menus" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureMenus.class"/>
	    </patternset>
	</testset>
    </testbag>
    
    <!-- 3rd round -->    
    <testbag testattribs="(measure_ui or measure_actions) and NOT measure_once" prio="8"
             executor="ide_performance" name="Measure UI responsiveness of actions" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureActions.class"/>
	    </patternset>
	</testset>
    </testbag>
    <testbag testattribs="(measure_ui or measure_dialogs) and NOT measure_once" prio="9"
             executor="ide_performance" name="Measure UI responsiveness of dialogs" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureDialogs.class"/>
	    </patternset>
	</testset>
    </testbag>
    <testbag testattribs="(measure_ui or measure_menus) and NOT measure_once" prio="10"
             executor="ide_performance" name="Measure UI responsiveness of menus" >
	<testset dir="qa-functional/src">
	    <patternset>
		<include name="gui/MeasureMenus.class"/>
	    </patternset>
	</testset>
    </testbag>
    
