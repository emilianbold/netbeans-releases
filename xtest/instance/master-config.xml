  <!-- PERFORMANCE -->
 
  <config name="qa-performance">
 	<module name="performance"   testtypes="unit"  attributes="measure_ide"/>  
        <module name="performance"   testtypes="qa-functional"  attributes="measure_ui"/>  
  </config>
  
  <config name="qa-performance-ide">
 	<module name="performance"   testtypes="unit"  attributes="measure_ide"/>  
  </config>
  
  <config name="qa-performance-ui">
        <module name="performance"   testtypes="qa-functional"  attributes="measure_ui"/>  
  </config>
  
  <config name="qa-web-performance">
 	<module name="web"   testtypes="qa-performance"  attributes="measure_ui"/>  
 	<module name="j2ee"   testtypes="qa-performance"  attributes="measure_all"/>  
 	<module name="performance"   testtypes="qa-functional"  attributes="measure_setup"/>  
  </config>
  
  <config name="qa-j2ee-functional">
 	<module name="j2ee"   testtypes="qa-functional"  attributes="validation"/>  
        <!-- <module name="serverplugins/jboss4" testtypes="qa-functional" attributes="dev-api"/>
        <module name="serverplugins/sun/appsrv81" testtypes="qa-functional" attributes="dev-api"/> -->
  </config>
  
  <!-- validation suites for Release Engineering -->

  <!-- validation suite before commit -->
  <config name="commit-validation-nb">
      <module name="core" testtypes="unit" attributes="commit"/>
      <module name="core/windows" testtypes="unit" attributes="commit" />
      <module name="core/javahelp" testtypes="unit" attributes="commit" />
      <module name="ide" testtypes="qa-functional" attributes="commit"/>
      <module name="editor" testtypes="qa-functional" attributes="commit" />
  </config>
  
  <config name="commit-validation-enterprise">
      <module name="enterprise/bpel" testtypes="qa-functional" attributes="commit"/>
      <module name="enterprise/xslt" testtypes="qa-functional" attributes="commit"/>
      <module name="xml/schema" testtypes="qa-functional" attributes="commit"/>
  </config>

  <config name="commit-validation-cnd">
      <module name="ide" testtypes="qa-functional" attributes="cnd"/>
  </config>
  
  <config name="commit-validation-profiler">
      <module name="ide" testtypes="qa-functional" attributes="profiler"/>
  </config>
  
  <config name="commit-validation-j2ee">
      <module name="ide" testtypes="qa-functional" attributes="j2ee"/>
  </config>

  <config name="commit-validation-mobility">
      <module name="mobility/project" testtypes="qa-functional" attributes="commit">
          <!-- Propagate wtk.dir property to build-qa-functional.xml. -->
          <property name="wtk.dir" value="${wtk.dir}"/>
      </module>
  </config>

  <config name="commit-validation-uml">
      <module name="uml" testtypes="qa-functional" attributes="commit"/>
  </config>

</testconfig>
