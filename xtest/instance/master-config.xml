<testconfig>

  <!-- Dev TESTS -->
  <!-- ========= -->  

    <config name="unit-nb"> <!-- was also similar to old "config-1" -->
        <module name="core" testtypes="unit" attributes="stable"/>
        <module name="openide" testtypes="unit" attributes="stable"/>
        <!-- Needs to be early since many other tests compile against it: -->
        <!-- (in general, check using: 'fgrep /build/test/unit/classes */{,*/}nbproject/project.properties') -->
        <module name="projects/projectapi" testtypes="unit" attributes="stable"/>
        <module name="ant" testtypes="unit" attributes="stable"/>
        <module name="ant/freeform" testtypes="unit" attributes="stable"/>
        <module name="ant/grammar" testtypes="unit" attributes="stable"/>
        <module name="ant/project" testtypes="unit" attributes="stable"/>
        <module name="apisupport/project" testtypes="unit" attributes="stable"/>
        <module name="java/api" testtypes="unit" attributes="stable"/>
        <module name="java/j2seplatform" testtypes="unit" attributes="stable"/>
        <module name="java/j2seproject" testtypes="unit" attributes="stable"/>
        <module name="java/platform" testtypes="unit" attributes="stable"/>
        <module name="java/project" testtypes="unit" attributes="stable"/>
        <module name="projects/projectui" testtypes="unit" attributes="stable"/>
        <module name="projects/libraries" testtypes="unit" attributes="stable"/>
        <!-- removed (not runnable): html, schema2beans -->
        <!-- other candidate modules in current default moduleconfig: autoupdate core/execution core/favorites core/multiview core/output2 core/settings core/windows debuggercore debuggerjpda editor editor/fold editor/util extbrowser httpserver j2eeserver java/javacore mdr openide/masterfs refactoring tasklist tasklist/core tasklist/docscan tasklist/suggestions tasklist/usertasks tomcatint/tomcat5 web/core web/jspparser web/webapi xml/api xml/core xml/schema xml/text-edit xml/xsl -->
    </config>
  
  <config name="unit-nb-nbm">
      <module name="mdr" testtypes="unit" attributes="stable"/>
      <module name="mdr" testtypes="perf" attributes="stable">
          <property file="../../mdr/test/perf.properties"/>
      </module>
  </config>
  
  <!-- QA TESTS -->
  <!-- ======== -->
  
  <!-- VALIDATION -->
  
  <!-- QA functional tests for Netbeans -->
  <config name="qa-functional-nb">
      <module name="beans"  	testtypes="qa-func"        attributes="validation"/>
      <testtype name="qa-functional" attributes="validation"
                modules="ant,clazz,core,editor,form,i18n,jarpackager,java,javacvs,javadoc,openide,properties,ide,vcscore,vcsgeneric"/>
      <testtype name="qa-functional" attributes="validation"
                modules="xml,xml/catalog,xml/core,xml/text-edit,xml/tools,xml/tax/lib,xml/tree-edit"/>
      <module name="qa"       	testtypes="serialization"  attributes="validation"/>
      <module name="debuggercore" testtypes="qa-functional" attributes="validation"/>
      <module name="debuggerjpda" testtypes="qa-unit" attributes="full"/>

      <!-- no longer in validation suite
      <module name="corba"   	testtypes="qa-functional"  attributes="validation"/>   -->
      <!-- no longer in validation suite
      <module name="projects" 	testtypes="qa-functional"  attributes="validation,jdk14"/>  -->
      <!-- it freezes
      module name="autoupdate" testtypes="gui"  	   attributes="validation"/ -->
  </config>
  
  <!-- QA functional tests for Netbeans on jdk 1.3 -->
  <config name="qa-functional-nb-jdk13">
      <module name="beans"  	testtypes="qa-func"        attributes="validation"/>
      <testtype name="qa-functional" attributes="validation"
                modules="ant,clazz,core,editor,form,i18n,jarpackager,java,javacvs,javadoc,openide,properties,vcscore,vcsgeneric"/>
      <testtype name="qa-functional" attributes="validation"
                modules="xml,xml/catalog,xml/core,xml/text-edit,xml/tools,xml/tax/lib,xml/tree-edit"/>
      <module name="qa"       	testtypes="serialization"  attributes="validation"/>  
      <module name="debuggercore"      testtypes="qa-gui"  attributes="validation"/>
  
      <!-- no longer in validation suite
      <module name="corba"   	testtypes="qa-functional"  attributes="validation"/>   -->
      <!-- no longer in validation suite
      <module name="projects" 	testtypes="qa-functional"  attributes="validation,jdk13"/>  -->
      <!-- temporarily removed because it crashes all testing
      <module name="ide"       	testtypes="qa-functional"  attributes="validation"/>    -->
      <!-- it freezes
      module name="autoupdate" testtypes="gui"  	   attributes="validation"/ -->
  </config>

  <!-- FULL -->
  
  <!-- QA functional tests for Netbeans -->
  <config name="qa-functional-nb-full">
      <module name="beans"  	testtypes="qa-func"        attributes="validation,full"/>
      <testtype name="qa-functional" attributes="validation,full"
                modules="ant,clazz,core,editor,form,i18n,jarpackager,java,javacvs,javadoc,junit,openide,properties,ide,vcscore,vcsgeneric"/>
      <testtype name="qa-functional" attributes="validation,full"
                modules="xml,xml/catalog,xml/core,xml/text-edit,xml/tools,xml/tax/lib,xml/tree-edit"/>
      <module name="jndi"    	testtypes="qa-functional"  attributes="validation,full">
          <property file="../../../nbextra/qa/jndi/jndi.properties"/>
      </module>      
      <module name="qa"       	testtypes="serialization"  attributes="validation,full"/>  
      <module name="debuggercore"      testtypes="qa-gui"  attributes="validation,full"/>
      <module name="debuggerjpda"      testtypes="qa-unit"  attributes="full"/>
      <module name="projects" 	testtypes="qa-functional"  attributes="validation,full,jdk14"/>

      <!-- module name="autoupdate" testtypes="gui"  	   attributes="validation,full"/ -->
      <module name="corba" 	testtypes="qa-functional"  attributes="validation,full"/>
  </config>
  
  <!-- QA functional tests for Netbeans on jdk 1.3 -->
  <config name="qa-functional-nb-full-jdk13">
      <module name="beans"  	testtypes="qa-func"        attributes="validation,full"/>
      <testtype name="qa-functional" attributes="validation,full"
                modules="ant,clazz,core,editor,form,i18n,jarpackager,java,javacvs,javadoc,junit,openide,properties,vcscore,vcsgeneric"/>
      <testtype name="qa-functional" attributes="validation,full"
                modules="xml,xml/catalog,xml/core,xml/text-edit,xml/tools,xml/tax/lib,xml/tree-edit"/>
      <module name="jndi"    	testtypes="qa-functional"  attributes="validation,full">
          <property file="../../../nbextra/qa/jndi/jndi.properties"/>
      </module>      
      <module name="qa"       	testtypes="serialization"  attributes="validation,full"/>  
      <module name="debuggercore"      testtypes="qa-gui"  attributes="validation,full"/>
      <module name="projects" 	testtypes="qa-functional"  attributes="validation,full,jdk13"/> 

      <!-- temporarily removed because it crashes all testing
      <module name="ide"       	testtypes="qa-functional"  attributes="validation,full"/> -->
      <!-- module name="autoupdate" testtypes="gui"  	   attributes="validation,full"/ -->
      <module name="corba" 	testtypes="qa-functional"  attributes="validation,full"/>
  </config>
  
  <!-- QA functional tests for FFJ EE -->
  <config name="qa-functional-ffjee">
      <module name="beans"  	testtypes="qa-func"        attributes="validation,full"/>
      <testtype name="qa-functional" attributes="validation,full"
                modules="ant,clazz,core,editor,form,i18n,jarpackager,java,javacvs,javadoc,junit,openide,properties,ide,vcscore,vcsgeneric"/>
      <testtype name="qa-functional" attributes="validation,full"
                modules="xml,xml/catalog,xml/core,xml/text-edit,xml/tools,xml/tax/lib,xml/tree-edit"/>
      <module name="jndi"    	testtypes="qa-functional"  attributes="validation,full">
          <property file="../../../nbextra/qa/jndi/jndi.properties"/>
      </module>      
      <module name="qa"       	testtypes="serialization"  attributes="validation,full"/>  
      <module name="debuggercore"      testtypes="qa-gui"  attributes="validation,full"/>
      <module name="projects" 	testtypes="qa-functional"   attributes="validation,full,jdk14"/>
      <module name="corba" 	testtypes="qa-functional"  attributes="validation,full"/>
  </config>
  
  <!-- QA functional tests for FFJ EE on jdk 1.3 -->
  <config name="qa-functional-ffjee-jdk13">
      <module name="beans"  	testtypes="qa-func"        attributes="validation,full"/>
      <testtype name="qa-functional" attributes="validation,full"
                modules="ant,clazz,core,editor,form,i18n,jarpackager,java,javacvs,javadoc,junit,openide,properties,rmi,vcscore,vcsgeneric"/>
      <testtype name="qa-functional" attributes="validation,full"
                modules="xml,xml/catalog,xml/core,xml/text-edit,xml/tools,xml/tax/lib,xml/tree-edit"/>
      <module name="jndi"    	testtypes="qa-functional"  attributes="validation,full">
          <property file="../../../nbextra/qa/jndi/jndi.properties"/>
      </module>      
      <module name="qa"       	testtypes="serialization"  attributes="validation,full"/>  
      <module name="debuggercore"      testtypes="qa-gui"  attributes="validation,full"/>
      <module name="projects" 	testtypes="qa-functional"   attributes="validation,full,jdk13"/>

      <!-- removed because it falls with OutOfMemoryError
      <module name="ide"       	testtypes="qa-functional"   attributes="validation,full"/>
      -->
      <module name="corba" 	testtypes="qa-functional"  attributes="validation,full"/>
  </config>
  
  <!-- QA functional tests for FFJ EE -->
  <config name="qa-functional-ffjee-validation">
      <module name="beans"  	testtypes="qa-func"        attributes="validation"/>
      <testtype name="qa-functional" attributes="validation"
                modules="ant,clazz,core,editor,form,i18n,jarpackager,java,javacvs,javadoc,junit,openide,properties,ide,vcscore,vcsgeneric"/>
      <testtype name="qa-functional" attributes="validation"
                modules="xml,xml/catalog,xml/core,xml/text-edit,xml/tools,xml/tax/lib,xml/tree-edit"/>
      <module name="qa"       	testtypes="serialization"  attributes="validation"/>  
      <module name="debuggercore"      testtypes="qa-gui"  attributes="validation"/>
  </config>
  
  <!-- QA functional tests for FFJ EE on jdk 1.3 -->
  <config name="qa-functional-ffjee-validation-jdk13">
      <module name="beans"  	testtypes="qa-func"        attributes="validation"/>
      <testtype name="qa-functional" attributes="validation"
                modules="ant,clazz,core,editor,form,i18n,jarpackager,java,javacvs,javadoc,junit,openide,properties,rmi,vcscore,vcsgeneric"/>
      <testtype name="qa-functional" attributes="validation"
                modules="xml,xml/catalog,xml/core,xml/text-edit,xml/tools,xml/tax/lib,xml/tree-edit"/>
      <module name="qa"       	testtypes="serialization"  attributes="validation"/>  
      <module name="debuggercore"      testtypes="qa-gui"  attributes="validation"/>

      <!-- removed because it falls with OutOfMemoryError
      <module name="ide"       	testtypes="qa-functional"   attributes="validation"/>
      -->
  </config>

  <config name="qa-performance">
 	<module name="performance"   testtypes="unit"  attributes="measure_ide"/>  
        <module name="performance"   testtypes="qa-functional"  attributes="measure_ui"/>  
  </config>
  
  <config name="qa-performance-ide">
 	<module name="performance"   testtypes="unit"  attributes="measure_ide"/>  
  </config>
  
  <config name="qa-performance-ui">
        <module name="performance"   testtypes="qa-functional"  attributes="measure_ui"/>  
  </config>
  
  <config name="qa-web-performance">
 	<module name="web"   testtypes="qa-performance"  attributes="measure_ui"/>  
  </config>
  
  <config name="david">
      <module name="jndi"    	testtypes="qa-functional"  attributes="validation">
          <property file="../../../nbextra/qa/jndi/jndi.properties"/>
      </module>
  </config> 

  <config name="test">
      <module name="vcsgeneric"	testtypes="qa-functional"   attributes="validation"/>      
  </config> 
  
  
  <!-- validation suites for Release Engineering -->

  <!-- validation suite for JDK 1.4 -->
  <config name="re-validation">
      <!-- developer's unit tests -->
      <module name="core" testtypes="unit" attributes="stable"/>
      <module name="openide" testtypes="unit" attributes="stable"/>
      <!-- qa's functional tests -->
      <module name="ide"       	testtypes="qa-functional"  attributes="validation"/>
  </config>
  
  <config name="re-validation-original">
      <!-- developer's unit tests -->
      <module name="core" testtypes="unit" attributes="stable"/>
      <module name="java" testtypes="unit" attributes="stable"/>
      <module name="openide" testtypes="unit" attributes="stable"/>
      <!-- qa's functional tests -->
      <module name="ant"    	testtypes="qa-functional"  attributes="validation"/>
      <module name="beans"  	testtypes="qa-func"        attributes="validation"/>
      <module name="clazz"  	testtypes="qa-functional"  attributes="validation"/>
      <module name="core"   	testtypes="qa-functional"  attributes="validation"/>  
      <module name="editor" 	testtypes="qa-functional"  attributes="validation"/>
      <module name="form" 	testtypes="qa-functional"  attributes="validation"/>
      <module name="i18n"   	testtypes="qa-functional"  attributes="validation"/>
      <module name="jarpackager"  	testtypes="qa-functional"  attributes="validation"/>
      <module name="java"   	testtypes="qa-functional"  attributes="validation"/>
      <module name="openide"  	testtypes="qa-functional"  attributes="validation"/>
      <module name="qa"       	testtypes="serialization"  attributes="validation"/>
      <module name="ide"       	testtypes="qa-functional"  attributes="validation"/>
      <module name="vcscore"	testtypes="qa-functional"  attributes="validation"/>      
      <module name="vcsgeneric"	testtypes="qa-functional"  attributes="validation"/>      
      <module name="xml"	testtypes="qa-functional"  attributes="validation"/>
      <module name="xml/catalog"	testtypes="qa-functional"  attributes="validation"/>
      <module name="xml/core"		testtypes="qa-functional"  attributes="validation"/>
      <module name="xml/text-edit"	testtypes="qa-functional"  attributes="validation"/>
      <module name="xml/tools"		testtypes="qa-functional"  attributes="validation"/>
      <module name="xml/tax/lib"	testtypes="qa-functional"  attributes="validation"/>
      <module name="xml/tree-edit"	testtypes="qa-functional"  attributes="validation"/>
  </config>  
  
  <!-- validation suite for all XML modules -->
  <config name="all-xml">
      <module name="xml"	testtypes="qa-functional"  attributes="full"/>
      <module name="xml/catalog"	testtypes="qa-functional"  attributes="full"/>
      <module name="xml/core"		testtypes="qa-functional"  attributes="full"/>
      <module name="xml/text-edit"	testtypes="qa-functional"  attributes="full"/>
      <module name="xml/tools"		testtypes="qa-functional"  attributes="full"/>
      <module name="xml/tax/lib"	testtypes="qa-functional"  attributes="full"/>
      <module name="xml/tree-edit"	testtypes="qa-functional"  attributes="full"/>
  </config>  

  <!-- validation suite before commit -->
  <config name="commit-validation-nb">
      <module name="core" testtypes="unit" attributes="commit"/>
      <module name="core/windows" testtypes="unit" attributes="commit" />
      <module name="openide" testtypes="unit" attributes="commit"/>
      <module name="projects/projectapi" testtypes="unit" attributes="commit"/>
      <module name="ant/project" testtypes="unit" attributes="commit"/>
      <module name="java/api" testtypes="unit" attributes="commit"/>
      <module name="java/j2seplatform" testtypes="unit" attributes="commit"/>
      <module name="java/platform" testtypes="unit" attributes="commit"/>
      <module name="java/j2seproject" testtypes="unit" attributes="commit"/>
      <module name="ide" testtypes="qa-functional" attributes="commit"/>
  </config>

</testconfig>

