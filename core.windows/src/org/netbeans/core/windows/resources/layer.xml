                </file>
                <file name="DTD_Window_Manager_2_2" url="nbres:/org/netbeans/core/windows/resources/windowmanager-properties2_2.dtd">
                    <attr name="hint.originalPublicID" stringvalue="-//NetBeans//DTD Window Manager 2.2//EN"/>
                </file>
            </folder>
    	</folder>
    </folder>

</filesystem>
