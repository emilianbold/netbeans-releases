<netbeans-deployment>
    
    <icon>org/netbeans/modules/tomcat5/resources/tomcat5</icon>
    
    <!--
    The disconnected-string element contains the String parameter to DeploymentFactory55.getDisconnectedDeploymentManager()
    -->
    <disconnected-string>tomcat55:jakarta-tomcat-5.5.x</disconnected-string>
    
    <container-limitation><war-deploy/></container-limitation>
    
    <web-context-root>
        <xpath>/</xpath>
        <prop-name>contextRoot</prop-name>
    </web-context-root>
</netbeans-deployment>
