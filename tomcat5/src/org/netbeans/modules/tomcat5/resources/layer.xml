<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.0//EN" "http://www.netbeans.org/dtds/filesystem-1_0.dtd">

<filesystem>

    <folder name="J2EE">
        <folder name="DeploymentPlugins">
            <folder name="Tomcat5">
                <file name="Descriptor" url="deployment.xml"/>
                <file name="Factory.instance">
                    <attr name="instanceCreate" methodvalue="org.netbeans.modules.tomcat5.TomcatFactory.create"/>
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.tomcat5.TomcatFactory"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.tomcat5.TomcatFactory,javax.enterprise.deploy.spi.factories.DeploymentFactory"/>
                </file>
                <file name="TomcatRegistryNodeFactory.instance">                                
                    <!--<attr name="instanceCreate" methodvalue="org.netbeans.modules.tomcat5.nodes.TomcatRegistryNodeFactory.create"/>-->
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.tomcat5.nodes.TomcatRegistryNodeFactory"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.api.RegistryNodeFactory"/>
                </file>
                <file name="OptionalFactory.instance">
                    <attr name="instanceCreate" newvalue="org.netbeans.modules.tomcat5.ide.OptionalFactory"/>
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.tomcat5.ide.OptionalFactory"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.api.OptionalDeploymentManagerFactory"/>
                </file>
                <file name="TomcatPlatformFactory.instance">
                    <attr name="instanceCreate" newvalue="org.netbeans.modules.tomcat5.ide.TomcatPlatformFactory"/>
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.tomcat5.ide.TomcatPlatformFactory"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.api.J2eePlatformFactory"/>
                </file>
                <file name="ConfigurationSupportImpl.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.tomcat5.ide.ConfigurationSupportImpl"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.api.ConfigurationSupport"/>
                </file>

                <folder name="DeploymentFileNames">
                    <folder name="WAR">
                        <file name="META-INF\context.xml"/>
                    </folder>
                </folder>
            </folder>
            <folder name="Tomcat55">
                <file name="Descriptor" url="deployment55.xml"/>
                <file name="Factory55.instance">
                    <attr name="instanceCreate" methodvalue="org.netbeans.modules.tomcat5.TomcatFactory55.create"/>
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.tomcat5.TomcatFactory55"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.tomcat5.TomcatFactory55,javax.enterprise.deploy.spi.factories.DeploymentFactory"/>
                </file>
                <file name="TomcatRegistryNodeFactory.instance">                                
                    <!--<attr name="instanceCreate" methodvalue="org.netbeans.modules.tomcat5.nodes.TomcatRegistryNodeFactory.create"/>-->
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.tomcat5.nodes.TomcatRegistryNodeFactory"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.api.RegistryNodeFactory"/>
                </file>
                <file name="OptionalFactory55.instance">
                    <attr name="instanceCreate" newvalue="org.netbeans.modules.tomcat5.ide.OptionalFactory55"/>
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.tomcat5.ide.OptionalFactory55"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.api.OptionalDeploymentManagerFactory"/>
                </file>
                <file name="TomcatPlatformFactory.instance">
                    <attr name="instanceCreate" newvalue="org.netbeans.modules.tomcat5.ide.TomcatPlatformFactory"/>
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.tomcat5.ide.TomcatPlatformFactory"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.api.J2eePlatformFactory"/>
                </file>
                <file name="ConfigurationSupportImpl.instance">
                    <attr name="instanceClass" stringvalue="org.netbeans.modules.tomcat5.ide.ConfigurationSupportImpl"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.j2ee.deployment.plugins.api.ConfigurationSupport"/>
                </file>
                <folder name="DeploymentFileNames">
                    <folder name="WAR">
                        <file name="META-INF\context.xml"/>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>
    
    <folder name="Services">

        <!-- Recognize text/tomcat50+xml: -->
        <folder name="MIMEResolver">
          <file name="org-netbeans-modules-tomcat5-resolver.xml" url="mime-resolver.xml">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.tomcat5.resources.Bundle"/>
            <!--<attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/tomcat5/resources/Tomcat40Icon.gif"/>-->
          </file>
        </folder>
        <folder name="JavaHelp">
          <!-- Merge after Core IDE Help: -->
          <attr name="org-netbeans-modules-usersguide-above-regular.txt/org-netbeans-modules-tomcat5-helpset.xml" boolvalue="true"/>
          <!-- Merge after Web Services Help: -->
          <attr name="com-sun-forte4j-webdesigner-docs-helpset.xml/org-netbeans-modules-tomcat5-helpset.xml" boolvalue="true"/>
          <file name="org-netbeans-modules-tomcat5-helpset.xml" url="helpset-decl.xml"/>
          <!-- Merge before 3rd party help: -->
          <attr name="org-netbeans-modules-tomcat5-helpset.xml/org-netbeans-modules-usersguide-below-regular.txt" boolvalue="true"/>
        </folder>

    </folder>
    
   

