    <xs:complexType name="tAddress">
       <xs:complexContent>
            <xs:extension base="wsdl:tExtensibilityElement">
                <xs:attribute name="url" type="xs:string" use="optional" default="ftp://anonymous:user\@yahoo.com@localhost:21">
